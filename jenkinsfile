pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
                git 'https://github.com/Pritam-Khergade/student-ui.git'
            }
        }
	stage('build') {
            steps {
                sh 'mvn clean package'
            }
        }
	stage('test') {
            steps {
                sh 'mvn sonar:sonar \
  -Dsonar.projectKey=studentapp1 \
  -Dsonar.host.url=http://34.224.26.192:9000 \
  -Dsonar.login=sqp_ed7f921d92988999245caed34e08e2af646a3f6a'
            }
        }
	stage('deploy') {
            steps {
                echo 'deploy success'
            }
        }

    }
}
