pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
                git 'https://github.com/Pritam-Khergade/student-ui.git'
            }
        }
	stage('build') {
            steps {
                sh 'mvn clean package'
            }
        }
	stage('test') {
            steps {
		withSonarQubeEnv(installationName: 'sonar', credentialsId: 'sonar') {
  			 sh 'mvn sonar:sonar '
		}	
                
            }
        }
	stage('deploy') {
            steps {
                echo 'deploy success'
            }
        }

    }
}
